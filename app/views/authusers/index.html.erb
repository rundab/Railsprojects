<p id="notice"><%= notice %></p>

<h1>Listing Authusers</h1>

<table>
  <% @authusers.each do |authuser| %>
      <tr class="<%= cycle('list_line_odd','list_line_even') %>">
        <td class="list_description">
          <dl>
            <dt><%= authuser.name %></dt>
          </dl>
        </td>
        <td class="list_actions" >
          <%= link_to 'Show', authuser %><br/>
          <%= link_to 'Edit', edit_authuser_path(authuser) %><br/>
          <%= link_to 'Destroy', authuser, method: :delete, data: {confirm: 'Are You Sure?' } %>
        </td>
      </tr>
  <% end %>
</table>

<br>

<%= link_to 'New Authuser', new_authuser_path %>
